{
    "name": "brainydigital/intima.ai-sdk-php",
    "description": "Implementação da API do Intima.ai em forma de SDK Client para PHP.",
    "keywords": [
        "intimai",
        "intima.ai",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://app.intima.ai",
    "license": "MIT",
    "authors": [
        {
            "name": "Rauny Henrique",
            "email": "rauny22@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    }
}
