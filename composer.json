{
    "name": "brainydigital/intima.ai-sdk-php",
    "description": "Implementação da API do Intima.ai em forma de SDK Client para PHP.",
    "keywords": [
        "intimai",
        "intimaai",
        "intima.ai",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://intima.ai",
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Galdino",
            "email": "eu@pedrogaldino.com",
            "role": "CTO"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": { "Intimaai\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Intimaai\\" : "test/" }
    }
}
